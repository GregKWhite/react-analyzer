{
  "name": "@artisanaljorts/react-analyzer",
  "version": "1.0.0",
  "description": "A static React analyzer - easily understand your React codebase",
  "targets": {
    "index": {
      "source": "src/index.ts",
      "context": "node",
      "distDir": "./dist",
      "isLibrary": true,
      "outputFormat": "commonjs"
    },
    "worker": {
      "source": "src/worker.ts",
      "context": "node",
      "distDir": "./dist",
      "isLibrary": true,
      "outputFormat": "commonjs"
    }
  },
  "bin": {
    "react-analyzer": "dist/index.js"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "parcel build --no-cache",
    "start": "pnpm build ; node --es-module-specifier-resolution=node dist/index.js"
  },
  "author": "Greg White",
  "license": "ISC",
  "dependencies": {
    "@ast-grep/napi": "^0.22.3",
    "@types/estraverse": "^5.1.1",
    "@types/estree": "0.0.51",
    "@types/estree-jsx": "0.0.1",
    "@typescript-eslint/typescript-estree": "^5.11.0",
    "astray": "^1.1.1",
    "async-sema": "^3.1.1",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "escodegen": "git+ssh://git@github.com/GregKWhite/escodegen.git",
    "glob": "^7.2.0",
    "ts-morph": "^22.0.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "os-browserify": "^0.3.0",
    "parcel": "^2.4.1",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4"
  }
}
